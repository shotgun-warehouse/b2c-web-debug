  "scripts": {
    "start": "next",
    "build": "next build",
    "start:prod": "next start",
    "test": "jest",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "apollo:codegen:gen": "apollo client:codegen --outputFlat --target typescript --addTypename --globalTypesFile ./AppApolloGlobalTypes.ts __genTypes__",
    "apollo:codegen:clean": "rimraf './**/__genTypes__'",
    "apollo:codegen:gitAdd": "git add __genTypes__/*.ts **/__genTypes__/*.ts",
    "apollo:codegen": "yarn run apollo:codegen:clean && yarn run apollo:codegen:gen && yarn run apollo:codegen:gitAdd",
    "apollo:codegen:fix": "rimraf node_modules && yarn install && yarn apollo:codegen",
    "deploy:staging": "now -A now.staging.json --target production",
    "deploy:prod": "now -A now.prod.json  --target production",
    "dev": "next",
    "now-dev": "now dev -A now.dev.json"
  },
  "engines": {
    "node": "10.x"
  },
